{
  "compilerOptions": {
    "outDir": "./functions",
    "target": "esnext",
    "module": "esnext",
    "lib": ["esnext"],
    "alwaysStrict": true,
    "strict": true,
    "preserveConstEnums": true,
    "moduleResolution": "node",
    "sourceMap": true,
    "esModuleInterop": true,
    "types": [
      "@cloudflare/workers-types",
      "@types/service-worker-mock"
    ]
  },
  "include": ["functions-src/**/*"],
  "exclude": ["functions/**/*"]
}
